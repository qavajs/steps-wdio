{
  "name": "@qavajs/steps-wdio",
  "version": "0.32.0",
  "description": "steps to interact with wdio",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:e2e": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/webui.ts",
    "debug:e2e": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/webui.ts --profile debug",
    "test:e2e:selenium": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/webui.ts --profile selenium",
    "debug:e2e:selenium": "ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config test-e2e/webui.ts --profile debugSelenium"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qavajs/steps-wdio.git"
  },
  "authors": [
    "Alexandr Galichenko",
    "Alexandr Legchilov"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qavajs/steps-wdio/issues"
  },
  "homepage": "https://github.com/qavajs/steps-wdio#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^9.1.2",
    "@qavajs/cli": "^0.26.0",
    "@qavajs/console-formatter": "^0.2.1",
    "@qavajs/memory": "^1.5.0",
    "@qavajs/po": "^1.4.0",
    "@qavajs/wdio-service-adapter": "^0.0.3",
    "@qavajs/webstorm-adapter": "^8.0.0",
    "@qavajs/xunit-formatter": "^0.0.4",
    "@types/chai": "^4.3.5",
    "@wdio/selenium-standalone-service": "^8.11.0",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@qavajs/validation": "^0.0.5",
    "webdriverio": "^8.11.1"
  }
}
